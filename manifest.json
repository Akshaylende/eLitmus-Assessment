{
    "name": "eLitmus Assessment Helper",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A Chrome extension to assist with eLitmus assessments.",
    "permissions": [
      "activeTab",
      "storage",
      "videoCapture",
      "audioCapture",
      "nativeMessaging"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "eLitmus Assessment Helper"
    },
    "icons": {
    }
  }
  